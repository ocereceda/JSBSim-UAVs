	  <delay>1.0</delay>
      <set name="ap/altitude_setpoint" value="1000.0"/>
      <set name="ap/altitude_hold" value="1"/>
	  <set name="ap/attitude_hold" value="0"/>
	  <set name="guidance/target_wp_latitude_rad" value="0.864541199"/>
	  <set name="guidance/target_wp_longitude_rad" value="-0.95559796"/>
