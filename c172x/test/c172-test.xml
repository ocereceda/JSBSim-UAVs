	<event name="Start the path again(WPA)">
      <description>Set Autopilot for heading to WPA </description>
      <condition> 	guidance/wp-distance lt 100 
					ap/active-waypoint eq 8
					navigation/actual-heading-rad gt 3.1416					</condition>
	  <delay>1.0</delay>
      <set name="ap/altitude_setpoint" value="1000.0"/>
      <set name="ap/altitude_hold" value="1"/>
	  <set name="ap/attitude_hold" value="0"/>
	  <set name="guidance/target_wp_latitude_rad" value="0.863098771"/>
	  <set name="guidance/target_wp_longitude_rad" value="-0.948470489"/>
      <set name="ap/heading-setpoint-select" value="1"/>
      <set name="ap/heading_hold" value="1"/>
	  <set name="guidance/heading-selector-switch" value="0"/>
